server {

    listen		*:80;
    server_name		elu_dia_w2ui_template.mydomain;

    root		/var/projects/elu_dia_w2ui_template/front/root;

    access_log		/var/log/nginx/elu_dia_w2ui_template.access.log eludia_requests;
    error_log		/var/log/nginx/elu_dia_w2ui_template.error.log;

    include		/var/projects/elu_dia_w2ui_template/front/conf/app.conf;

}